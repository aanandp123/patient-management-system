<div class="row">
  <div class="col-sm-8">
    <div class="card">
      <div class="card-body">

      <form action ="/Luser" method="get">
        <%for (var i =0;i<person.exercise.length;i++){%>
          <div class="form-group">
            <label for="text">Name: <%= person.exercise[i]%></label>
          </div>

          <div class="form-group">
            <label for="text"> Description: <%= person.exercisedesc[i]%></label>
          </div>
        <%}%>
